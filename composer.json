{
  "name": "ndestates/craftcms-nde",
  "description": "Craft CMS website for realtor",
  "version": "1.0.1",
  "require": {
  "bolden/google-maps-api": "1.0.3",
  "craftcms/cms": "3.5.16",
  "craftcms/element-api": "2.6.0",
  "craftcms/feed-me": "4.3.3",
  "craftcms/redactor": "2.8.5",
  "enupal/slider": "^1.3",
  "hillholliday/craft-user-manual": "^2.0",
  "mmikkel/reasons": "^2.2",
  "nystudio107/craft-imageoptimize": "1.6.20",
  "nystudio107/craft-retour": "^3.1",
  "nystudio107/craft-seomatic": "3.3.26",
  "nystudio107/craft-twigpack": "^1.2.7",
  "nystudio107/craft-typogrify": "^1.1.18",
  "nystudio107/craft-minify": "^1.2.0",
  "ostark/craft-async-queue": "^2",
  "topshelfcraft/scraper": "3.0.0",
  "topshelfcraft/supersort": "^3.1",
  "yiisoft/yii2-redis": "^2.0.14",
  "verbb/navigation": "1.4.8",
  "vlucas/phpdotenv": "^3.4.0"
},
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
